		<Element Type="SqlProcedure" Name="[dbo].[GetOrdersByStatus]">
			<Property Name="BodyScript">
				<Value><![CDATA[
BEGIN
--Testing
    SELECT OrderStatus, COUNT(*) as OrdersCount FROM dbo.Orders
    WHERE StartDate BETWEEN @FromDate and @ToDate
    GROUP BY OrderStatus
END;]]></Value>
